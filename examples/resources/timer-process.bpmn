    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="wait-timer" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="wait-timer" targetRef="after-timer-task" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="after-timer-task" targetRef="EndEvent_1" />
  </bpmn:process>
